{
  "plugins": [
    "stylelint-scss"
  ],
  "extends": [
    "stylelint-config-standard"
  ],
  "customSyntax": "postcss-scss",
  "ignoreFiles": [
    "dist/**/*"
  ],
  "rules": {
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true,
    "selector-class-pattern": null,
    "no-empty-source": [
      true,
      {
        "severity": "warning"
      }
    ]
  }
}